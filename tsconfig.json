{
	"compilerOptions": {
		"baseUrl": ".",
		"allowJs": true,
		"esModuleInterop": true,
		"lib": ["ES2015", "ES2017"],
		"jsx": "preserve",
		"jsxFactory": "h",
		"jsxFragmentFactory": "Fragment",
		"moduleResolution": "Node",
		"skipLibCheck": true,
		"strict": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitReturns": true,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"target": "ESNext",
		"paths": {}
	}
}
